    "cadena1 = 'Hola'\n",
    "cadena2 = 'Mundo'\n",
    "def _medio (cadena1,cadena2):\n",
    "    posmedio = len(cadena1)//2\n",
    "    cadena3 = cadena1[:posmedio] + cadena2 + cadena1[posmedio:]\n",
    "    return cadena3\n",
    "\n",
    "cadenar= _medio(cadena1,cadena2)   \n",
    "print(f'La cadena final es: {cadenar}')"
